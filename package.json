    "scripts": {
        "babel": "mkdirp Scripts/build && babel --presets @babel/preset-env Scripts/src --out-dir Scripts/build/ --ignore Scripts/src/*/spec/",
        "build": "npm run babel && node Scripts/build/BuildScripts/BuildClients.js",
        "build_debug": "mkdirp Scripts/build  && babel --presets @babel/preset-env Scripts/src --out-dir Scripts/build/ --ignore Scripts/src/*/spec/ --source-maps --watch",
        "test": "node_modules/.bin/jest --runInBand --config jestconfig.json --silent",
        "coverage": "node_modules/.bin/jest --coverage --config jestconfig.json",
        "testwitdebug": "node --inspect-brk node_modules/.bin/jest --runInBand --config jestconfig.json",
        "create": "npm run babel && node Scripts/build/UtilityScripts/ClientCreation.js",
        "lint": "eslint .",
        "lint-fix": "eslint . --fix",
        "create-utv2": "npm run babel && node Scripts/build/UtilityScripts/UTVClientCreation.js"
    },
